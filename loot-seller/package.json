{
  "name": "loot-seller",
  "version": "1.0.0",
  "description": "A simple React application for searching and tracking loot items",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:single": "react-scripts build && node scripts/createSingleFile.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "scrape-items-and-images": "node scripts/scrapeItems.js",
    "scrape-items": "node scripts/scrapeItems.js --skip-images",
    "scrape-some-raids": "node scripts/scrapeRaids.js --limit=10",
    "scrape-raids": "node scripts/scrapeRaids.js",
    "scrape-mobs": "node scripts/scrapeMobs.js",
    "correct-raids": "node scripts/scrapeRaids.js --corrections-only"
  },
  "keywords": [
    "react",
    "loot",
    "search"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0",
    "clsx": "^2.1.1",
    "fuse.js": "^7.0.0",
    "lucide-react": "^0.511.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^3.3.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "glob": "^11.0.2",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.3"
  }
}
